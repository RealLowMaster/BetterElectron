	"version": "0.2.0",
	"configurations": [
		{
			"autoAttachChildProcesses": false,
			"enableDWARF": false,
			"internalConsoleOptions": "neverOpen",
			"postDebugTask": "disconnect()",
			"command": "npm run run",
			"name": "Run App",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"autoAttachChildProcesses": false,
			"enableDWARF": false,
			"name": "Compile App",
			"internalConsoleOptions": "neverOpen",
			"command": "npm run compile",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"autoAttachChildProcesses": false,
			"enableDWARF": false,
			"name": "Release App",
			"internalConsoleOptions": "neverOpen",
			"command": "npm run release",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"autoAttachChildProcesses": false,
			"enableDWARF": false,
			"name": "Sync Themes",
			"internalConsoleOptions": "neverOpen",
			"command": "npm run theme-sync",
			"request": "launch",
			"type": "node-terminal"
		}
	]
}